{
	"name": "zkcloudworker-app",
	"version": "0.1.2",
	"description": "The zkCloudWorker App",
	"main": "dist/index.js",
	"homepage": "https://zkcloudworker.com/",
	"private": false,
	"license": "Apache-2.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"gen:exports": "svelte-lib-helpers exports",
		"gen:docs": "svelte-lib-helpers docs",
		"gen:compo-data": "svelte-lib-helpers compo-data",
		"copy:package": "svelte-lib-helpers package",
		"lib-helpers": "npm run gen:docs && npm run gen:compo-data && npm run build && npm run gen:exports && npm run copy:package",
		"package:publish": "standard-version && git push --follow-tags origin main && npm publish"
	},
	"devDependencies": {
		"@felte/validator-yup": "^1.1.3",
		"@playwright/test": "^1.42.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/enhanced-img": "^0.2.0",
		"@sveltejs/kit": "^2.5.3",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/color-hash": "^1.0.5",
		"@types/eslint": "8.56.0",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"autoprefixer": "^10.4.18",
		"dayjs": "^1.11.10",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"felte": "^1.2.14",
		"flowbite": "^2.3.0",
		"flowbite-svelte": "^0.44.24",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.12",
		"svelte": "^4.2.12",
		"svelte-awesome-icons": "^1.2.0",
		"svelte-check": "^3.6.6",
		"svelte-lib-helpers": "^0.3.17",
		"svelte-meta-tags": "^3.1.1",
		"svelte-tags-input": "^6.0.0",
		"svelte-time": "^0.8.3",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^5.1.5",
		"vitest": "^1.3.1",
		"yup": "^1.4.0"
	},
	"type": "module",
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"dependencies": {
		"@socialcap/contracts-lib": "^0.1.18",
		"@socialcap/payments": "^0.1.3",
		"@sveltejs/adapter-vercel": "^5.2.0",
		"@tanstack/svelte-query": "^5.29.0",
		"algoliasearch": "^4.23.3",
		"axios": "^1.7.2",
		"color-hash": "^2.0.2",
		"flowbite-svelte-icons": "^1.5.0",
		"nats.ws": "^1.27.0",
		"o1js": "^1.1.0",
		"slugify": "^1.6.6",
		"svelte-exmarkdown": "^3.0.3",
		"tailwind-merge": "^2.2.1",
		"zkcloudworker": "^0.8.2"
	},
	"keywords": [
		"Socialcap",
		"ui"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Socialcap-app/socialcap-ui"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	}
}
